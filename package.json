{
  "name": "monix",
  "version": "0.0.1",
  "description": "A Node.js Tool For Emulating Web APIs, easy but powerful.(基于NodeJS的Web API模拟工具，以最简单的方式实现强大的模拟和控制能力) ",
  "main": "./lib/index.js",
  "bin": {
    "monix": "./bin/monix.js"
  },
  "scripts": {
    "test": "make test",
    "build": "./node_modules/.bin/babel src -d lib",
    "watch": "./node_modules/.bin/babel src -d lib -w",
    "serve": "./node_modules/.bin/supervisor -w ./lib,./test/cli -x ./bin/monix.js test/cli/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filow/monix.git"
  },
  "keywords": [
    "nodejs",
    "tool",
    "emulator",
    "web",
    "api",
    "webapi"
  ],
  "author": "Filow Lee <dev@filowlee.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filow/monix/issues"
  },
  "homepage": "https://github.com/filow/monix#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-async-to-generator": "^6.5.0",
    "babel-preset-es2015-node4": "^2.0.3",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "estraverse-fb": "^1.3.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "should": "^8.2.2",
    "should-http": "0.0.4",
    "supertest": "^1.2.0",
    "supervisor": "^0.9.1",
    "test-console": "^1.0.0"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "chance": "^1.0.1",
    "commander": "^2.9.0",
    "koa": "^2.0.0-alpha.3",
    "lodash": "^4.6.1",
    "moment": "^2.12.0",
    "path-to-regexp": "^1.2.1",
    "randexp": "^0.4.2"
  }
